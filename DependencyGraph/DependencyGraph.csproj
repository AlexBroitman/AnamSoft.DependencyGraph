<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>DependencyGraph is a generic implementation of classic dependency graph with or without cyclic dependencies.</Description>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <RootNamespace>AnamSoft.DependencyGraph</RootNamespace>
    <Authors>Alex Broitman</Authors>
    <Company>AnamSoft</Company>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageId>AnamSoft.DependencyGraph</PackageId>
    <Copyright>AnamSoft</Copyright>
    <RepositoryUrl>https://github.com/AlexBroitman/AnamSoft.DependencyGraph</RepositoryUrl>
    <Nullable>enable</Nullable>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <RepositoryType>Public</RepositoryType>
    <PackageTags>DependencyGraph</PackageTags>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AnamSoft.ReadOnlySet" Version="1.0.0" />
  </ItemGroup>
</Project>
